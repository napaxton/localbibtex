	Pmid = {1085446984001205869related:bTbp70dIEA8J},
	Rating = {0},
	Title = {A history of state action: the politics of AIDS in Uganda and Senegal},
	Uri = {papers://AF6EC7BB-DE9C-436B-9745-1710B598B3B9/Paper/p7521},
	Url = {http://rds.refer.sn/sites/rds.refer.sn/IMG/pdf/16PUTZEL.pdf},
