@unpublished{lieberman2005,
	Abstract = {Within the span of a decade, the HIV/AIDS pandemic swept the African continent, making already tragic human conditions even worse. The severity of this contagious disease, and the shared international policy pressures it engendered, created a natural experiment for investigating the political and other determinants of policy responses. Subject to similar challenges, why have polities responded in different ways? In this paper, I investigate the impact of ethnic politics on policy. I propose a theory which specifies the influence of ethnicity on assessments of risk of contagion, with consequences for the political costs of government (in)action. Using two different measures of policy aggressiveness, I find through cross-country regression analyses that politically relevant ethnic diversity negatively influences government responsiveness to HIV/AIDS. This is particularly tragic for sub-Saharan Africa, where the epidemic is most severe, and where ethnicity is often a central component of political life. Nonetheless, as a discussion of the statistically outlying case of Uganda demonstrates, ethnicity is not destiny. 
},
	Author = {Lieberman, Evan S.},
	Date-Added = {2009-12-29 22:40:03 -0500},
