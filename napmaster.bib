	Date-Modified = {2009-12-29 22:40:03 -0500},
	Howpublished = {World Bank Manuscript},
	Month = {June},
	Pages = {118},
	Title = {The Long-run Economic Costs of {AIDS}: Theory and an Application to {S}outh {A}frica},
