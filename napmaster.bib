@unpublished{paxton2010b,
	Abstract = {This paper examines the relationship between political processes and the HIV/AIDS epidemic. It examines the relationship between political processes that affect citizen association, like democracy or various types of war, and the spread of HIV/AIDS. It hypothesizes that political circumstances that promote association will also raise HIV/AIDS levels in the affected countries, while anti-associative pro- cesses will restrain the spread of the disease. Using data sets com- mon in political science and international relations but not in more epidemiological sciences, I conduct time-series-cross-section, fixed- effects regression analysis. The statistics indicate that democracy and democratic transitions are associated with increased HIV/AIDS, while civil and ethnic warfare have lower associated levels of HIV.},
	Author = {Paxton, Nathan A.},
	Date-Added = {2011-02-01 21:00:33 -0500},
